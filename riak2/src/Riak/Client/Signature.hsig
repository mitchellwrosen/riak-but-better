signature Riak.Client.Signature where

import Riak.Message (Message)

import Prelude
import Network.Socket (HostName, PortNumber)

data Client

connect ::
     HostName
  -> PortNumber
  -> IO Client

disconnect ::
     Client
  -> IO ()

exchange ::
     Client
  -> Message
  -> IO (Maybe Message)

stream ::
     Client
  -> Message
  -> (IO (Maybe Message) -> IO r)
  -> IO r
